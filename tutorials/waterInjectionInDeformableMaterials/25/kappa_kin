FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "25";
    object      kappa_kin;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    innerWall
    {
        type            calculated;
        value           uniform 0;
    }
    outerWall
    {
        type            calculated;
        value           uniform 0;
    }
    frontAndBack
    {
        type            empty;
    }
}


