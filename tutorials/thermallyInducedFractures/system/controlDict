FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     hybridBiotThermalInterFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime; //writeNow; //endTime;

endTime         6000;

deltaT          1e-7;

writeControl    adjustableRunTime; //timeStep

writeInterval   500;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.1;
maxCoUs		0.1;
maxAlphaCo      0.1;
maxDeltaT       0.5;


